{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "declaration": false,
    "outDir": "dist",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "inlineSourceMap": true,
    "lib": [
      "es6",
      "esnext"
    ]
  },
  "include": [
    "src"
  ]
}